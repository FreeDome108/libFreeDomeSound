prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: FreeDomeSound
Description: FreeDome Sound processing library for immersive dome projection systems
Version: @PROJECT_VERSION@
Libs: -L${libdir} -lfreedomesound_core
Libs.private: -lavformat -lavcodec -lavutil -lswscale -lswresample -pthread
Cflags: -I${includedir}/freedomesound
